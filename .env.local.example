# Configuração local para desenvolvimento no VS Code
# Copie este arquivo para .env.local e configure com seus dados

# Database Configuration (Local PostgreSQL)
DATABASE_URL=postgresql://postgres:password@localhost:5432/snacks_chicken

# PostgreSQL Connection Details
PGHOST=localhost  
PGPORT=5432
PGUSER=postgres
PGPASSWORD=password
PGDATABASE=snacks_chicken

# Authentication (Desenvolvimento)
SESSION_SECRET=desenvolvimento-session-secret-key-123456
REPL_ID=local-development
REPLIT_DOMAINS=localhost:5000
ISSUER_URL=https://replit.com/oidc

# Environment
NODE_ENV=development
PORT=5000

# Debug (opcional)
DEBUG=express:*